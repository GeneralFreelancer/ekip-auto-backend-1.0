{
    "name": "lever",
    "version": "1.0.0",
    "main": "build/app.js",
    "scripts": {
        "build": "npm run clear && tsc && npm run static",
        "dev": "nodemon src/app.ts -L dev local",
        "dev:prod": "npm run secretes && nodemon src/app.ts -L prod",
        "dev:stage": "npm run secretes && nodemon src/app.ts -L stage local",
        "start": "node build/app.js",
        "stage": "npm run secretes && node build/app.js stage",
        "static": "cp -r ./src/static/ ./build/static",
        "clear": "rm -rf ./build",
        "secretes": "node src/secrets",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prepare": "husky install"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
