        "async": "^3.2.4",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "date-fns": "^2.29.3",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "helmet": "^6.0.1",
        "jsonwebtoken": "^9.0.0",
        "mongodb": "^5.1.0",
        "mongoose": "6.10.0",
